   "source": [
    "num = int(input(\"Please enter a number: \"))\n",
    "for i in range(2,num):\n",
    "    if num%i != 0:\n",
    "        if i == num-1:\n",
    "            print(f\"{num} is a prime number.\")\n",
    "            break\n",
    "    else:\n",
    "        print(f\"{num} is not a prime number.\")\n",
    "        break"
